<h3>Welcome <%= @current_user.name %></h3>
<div >
  <% @recipes.each do |recipe| %>
    <div class="rec-wrapper">
      <div class="rec-homepage-title">
        <%= recipe.title %>
      </div>
      <div class="rec-homepage-photo">
        <%= image_tag recipe.photo.url(:medium) %>
      </div>
    </div>
  <% end %>
</div>
